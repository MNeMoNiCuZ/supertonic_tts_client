# Supertonic TTS Client Configuration
# Copy this file to .env and customize as needed

# Server Settings
BASE_URL=http://localhost:8765
TIMEOUT=30

# Default Synthesis Settings, supports mp3 as format
DEFAULT_VOICE=M1
DEFAULT_QUALITY=5
DEFAULT_SPEED=1.05
DEFAULT_FORMAT=wav

# Path Settings
# Leave TEMP_DIR empty to use system default temp directory
TEMP_DIR=
OUTPUT_DIR=output

# Playback Settings
AUTO_CLEANUP_TEMP=true
